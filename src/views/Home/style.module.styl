.App
  text-align center

.content
  color red
.stark
  font-size 60p

.App-logo
  animation App-logo-spin infinite 20s linear
  height 8vmin
  pointer-events none


.App-header
  background-color #282c34
  min-height 100vh
  display flex
  flex-direction column
  align-items center
  justify-content center
  font-size calc(10px + 2vmin)
  color white


.App-link
  color #61dafb


@keyframes App-logo-spin
  from
    transform rotate(0deg)

  to
    transform rotate(360deg)


.home-box
  text-align center
  height 100%
  background: #fff
  padding: 150px;
  img
    width: 300px
    height: auto
    display: block
    margin: 0 auto
  h3
    line-height: 50px
